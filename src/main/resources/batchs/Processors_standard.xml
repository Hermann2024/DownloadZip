	<processor name="TrtBatchRun">
		<variables>
			<variable name="input">
				<value>
					<batchRunCriteriaListType>
						<list>
							<startDate>
								<beforeAge>86400</beforeAge>
							</startDate>
						</list>
					</batchRunCriteriaListType>
				</value>
			</variable>
			<variable name="response"/>
		</variables>
		<block>
			<service name="TrtBatchRun" operation="deleteBatchRunList" refservice="BatchRunManagerService">
				<params>
					<input/>
					<context variable="context"/>
				</params>
				<return variable="response"/>
			</service>
		</block>
		<output return="response"/>
	</processor>
  <processor name="ExtQuerySelect">
    <variables>
      <variable name="scenario">

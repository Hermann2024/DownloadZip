   <SQLQuery><![CDATA[
SELECT BRUNID, BRUNPATH, TRUNID, TRUNPATH, TAUD.id as TAUID, BRUNSITE, TRUNSITE, TAUD.SITE_CODE TAUSITE
FROM
(
SELECT BRUN.id as BRUNID, BRUN.BATCH_CODE as BRUNPATH , TRUN.id as TRUNID, TRUN.TASK_CODE as TRUNPATH,
       BRUN.SITE_CODE as BRUNSITE, TRUN.SITE_CODE as TRUNSITE
FROM batch_run BRUN, task_run TRUN
WHERE BRUN.id=TRUN.parent_id
) TRUNVIEW , TASK_AUDIT TAUD
   ]]></SQLQuery>

          Entity CDATA #REQUIRED
          SourceUsage CDATA #IMPLIED
          AssociationEnd CDATA #IMPLIED
          Association CDATA #IMPLIED
          ReadOnly  %Boolean;
          Reference %Boolean;
          EntireObjectTable %Boolean;
          >


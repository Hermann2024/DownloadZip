<!ATTLIST ValidationBean
          Res_Id       CDATA #IMPLIED
          Exposed      %Boolean;
          OnAttribute  CDATA #IMPLIED
          OperandType  CDATA #REQUIRED
          Inverse      %Boolean;
          Name         CDATA #IMPLIED
          BeanClass    CDATA #REQUIRED>



<!ELEMENT ViewAttribute (Properties?, Data?, DesignTime?) >
<!ATTLIST ViewAttribute
          Name              CDATA #REQUIRED
          AliasName         CDATA #IMPLIED
          EntityAttrName    CDATA #IMPLIED
          EntityUsage       CDATA #IMPLIED
          Expression        CDATA #IMPLIED
          Type              CDATA #IMPLIED
          ColumnType        CDATA #IMPLIED
          ElemType          CDATA #IMPLIED
          SQLType           CDATA #IMPLIED
          Domain            CDATA #IMPLIED
          IsUpdateable      %UpdateableFlag;
          IsSelected        %Boolean2;
          IsQueriable       %Boolean2;
          IsVisible         %Boolean2;
          IsPersistent      %Boolean2;
          IsUnique          %Boolean;
          IsNotNull         %Boolean;
          PrecisionRule     %Boolean;
          AttrLoad          %AttrLoadEnum;
          DiscrColumn       %Boolean;
          Passivate         %Boolean;
          Scale             CDATA "-127"
          Precision         CDATA "0"
          DefaultValue      CDATA #IMPLIED>


<!ELEMENT Attribute (Properties?, Data?, (DesignTime         |
                                RangeValidationBean   |
                                ValidationBean        |
                                MethodValidationBean  |
                                CompareValidationBean |
                                ListValidationBean)*)>
<!ATTLIST Attribute
          Name              CDATA #REQUIRED
          Type              CDATA #REQUIRED
          ElemType          CDATA #IMPLIED
          Domain            CDATA #IMPLIED
          IsUpdateable      %UpdateableFlag;
          IsSessionScope    %Boolean;
          IsSelected        %Boolean2;
          IsQueriable       %Boolean2;
          IsVisible         %Boolean2;
          IsPersistent      %Boolean2;
          IsUnique          %Boolean;
          IsNotNull         %Boolean;
          AttrLoad          %AttrLoadEnum;
          ColumnName        CDATA #REQUIRED
          ColumnType        CDATA #REQUIRED
          SQLType           CDATA #IMPLIED
          TableName         CDATA #IMPLIED
          PrimaryKey        %Boolean;
          DiscrColumn       %Boolean;
          OIDAttrNames      CDATA #IMPLIED
          ChangeIndicator   %Boolean;
          RetrievedOnUpdate %Boolean;
          RetrievedOnInsert %Boolean;
          HistoryColumn     %HistoryKind;
          Scale             CDATA "-127"
          Precision         CDATA "0"
          PrecisionRule     %Boolean2;
          Expression        CDATA #IMPLIED
          DefaultValue      CDATA #IMPLIED>

<!ELEMENT Variable (Properties?, Data?, (DesignTime         |
                                RangeValidationBean   |
                                ValidationBean        |
                                MethodValidationBean  |
                                CompareValidationBean |
                                AttrArray             |
                                ListValidationBean)*)>
<!ATTLIST Variable
          Name              CDATA #REQUIRED
          Type              CDATA #REQUIRED
          Kind              CDATA #IMPLIED
          IsUpdateable      %UpdateableFlag;
          IsNotNull         %Boolean;
          Scale             CDATA "-127"
          Precision         CDATA "0"
          PrecisionRule     %Boolean2;
          DefaultValue      CDATA #IMPLIED>


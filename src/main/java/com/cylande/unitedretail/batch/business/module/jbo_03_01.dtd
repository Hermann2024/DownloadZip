<!ELEMENT Attr EMPTY>
<!ATTLIST Attr
          Name     CDATA #REQUIRED
          Value    CDATA #REQUIRED>

<!ELEMENT Item EMPTY>
<!ATTLIST Item Value CDATA #REQUIRED>

<!ELEMENT SQLQuery ANY>

<!ELEMENT StringItem EMPTY>
<!ATTLIST StringItem Value CDATA #REQUIRED>

<!ELEMENT DesignTimeStringArray (StringItem+)>
<!ATTLIST DesignTimeStringArray Name CDATA #REQUIRED>

<!ELEMENT DesignTime (Attr | AttrArray)* >

<!ELEMENT ComponentInterfaceBase (Item)* >
<!ELEMENT RowInterfaceBase (Item)* >

<!ELEMENT Properties (Property+) >

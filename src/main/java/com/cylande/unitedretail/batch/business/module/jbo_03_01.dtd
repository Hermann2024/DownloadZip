          MaxActiveNodes  CDATA #IMPLIED
          RowClass           CDATA #IMPLIED
          ComponentClass     CDATA #IMPLIED
          MsgBundleClass     CDATA #IMPLIED
          DefClass        CDATA #IMPLIED
          ComponentInterface CDATA #IMPLIED
          ClientProxyName    CDATA #IMPLIED
          ClientRowProxyName CDATA #IMPLIED
          RowInterface       CDATA #IMPLIED
          FetchMode       CDATA #IMPLIED
          FetchSize       CDATA #IMPLIED
          MaxFetchSize    CDATA #IMPLIED
          Passivate        %VOPassivationEnum;
          PageIterMode     %PageIterModeType;
          ViewLinkAccessorRetained %Boolean;
          UseGlueCode %Boolean2;
          Extends         CDATA #IMPLIED
          IsFinderView  %Boolean;>



<!ELEMENT ViewUsage (Properties?, Data?, DesignTime?, Remote*)>
<!ATTLIST ViewUsage
          Name            CDATA #REQUIRED

          OnAttribute CDATA #IMPLIED>

<!ELEMENT RangeValidationBean ANY>
<!ATTLIST RangeValidationBean
          ResId      CDATA #IMPLIED
          Exposed     %Boolean;
          OnAttribute CDATA #IMPLIED
          OperandType CDATA #REQUIRED
          Inverse     %Boolean;
          MinValue    CDATA #REQUIRED
          MaxValue    CDATA #REQUIRED>

<!ELEMENT UniquePKValidationBean ANY>
<!ELEMENT CompareValidationBean ANY>
<!ATTLIST CompareValidationBean
          ResId       CDATA #IMPLIED
          Exposed      %Boolean;
          OnAttribute  CDATA #IMPLIED
          OperandType  CDATA #REQUIRED
          Inverse      %Boolean;
          CompareType  CDATA #REQUIRED
          CompareValue CDATA #REQUIRED>

<!ELEMENT ListValidationBean (AttrArray?)>
<!ATTLIST ListValidationBean
          ResId       CDATA #IMPLIED
          Exposed      %Boolean;
          OnAttribute  CDATA #IMPLIED
          OperandType  CDATA #REQUIRED
          Inverse      %Boolean;
          ListValue    CDATA #IMPLIED>

<!ELEMENT RegisteredRule ANY>

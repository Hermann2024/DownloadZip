<!ELEMENT ViewLinkAccessor ANY>
<!ATTLIST ViewLinkAccessor
          Name              CDATA #IMPLIED
          Type              CDATA #IMPLIED
          ViewLinkEnd       CDATA #IMPLIED
          ViewLinkOtherEnd  CDATA #IMPLIED
          ViewLink          CDATA #IMPLIED
          ViewLinkName      CDATA #IMPLIED
          Reversed          %Boolean;
          IsUpdateable      %UpdateableFlag;>

<!ELEMENT AccessorAttribute ANY>
<!ATTLIST AccessorAttribute
          Name              CDATA #IMPLIED
          Association       CDATA #IMPLIED
          Type              CDATA #IMPLIED
          AssociationEnd    CDATA #IMPLIED
          AssociationOtherEnd CDATA #IMPLIED
          IsUpdateable      %UpdateableFlag;>


                                          UniquePKValidationBean?,
                                         (ValidationBean         |
                                          RangeValidationBean    |
                                          MethodValidationBean   |
                                          CompareValidationBean  |
                                          ListValidationBean     |
                                          Attribute              |
                                          AccessorAttribute      |
                                          CustomAttribute        |
                                          AssociationAccessor    |
                                          ViewLinkAccessor       |
                                          Variable               |
                                          Publisher       |
                                          Key)* )>
<!ATTLIST Entity
          Name          CDATA #REQUIRED
          Extends       CDATA #IMPLIED
          DBObjectType  CDATA #IMPLIED
          DBObjectName  CDATA #IMPLIED
          ViewObjectBase CDATA #IMPLIED
          AliasName     CDATA #IMPLIED
          BindingStyle  CDATA #IMPLIED
          BatchThreshold CDATA #IMPLIED
          IsPersistent  %Boolean2;
          IsUpdatable   %Boolean2;
          IsRefPKBased  %Boolean;
          AssociationAccessorRetained %Boolean;
          UpdateChangedColumns %Boolean2;
          RowClass      CDATA #IMPLIED
          CollClass     CDATA #IMPLIED
          DefClass      CDATA #IMPLIED
          MsgBundleClass    CDATA #IMPLIED
          EJBObjectClassName CDATA #IMPLIED
          EJBHomeClassName   CDATA #IMPLIED
          EJBHomeJNDIName    CDATA #IMPLIED
          UseGlueCode %Boolean2; >


<!ELEMENT EntityUsage (DesignTime?, ViewAttribute*)>
<!ATTLIST EntityUsage
